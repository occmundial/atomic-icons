{
  "name": "atomic-icons",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir scripts",
    "test": "jest --silent",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "prebuild:sprite": "rimraf dist && mkdirp dist",
    "build:sprite": "svg-sprite-generate -d ./icons -o ./dist/sprite.svg",
    "postbuild:sprite": "svgo dist/sprite.svg -o dist/atomic-icons.svg && rimraf dist/sprite.svg"
  },
  "dependencies": {
    "@occmundial/occ-atomic": "^1.15.6",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-jss": "8.6.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/babel__core": "^7.1.15",
    "@types/node": "^16.0.1",
    "@types/react": "17.0.13",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.3",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "svg-sprite-generator": "^0.0.7",
    "svgo": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "eslint --fix"
  }
}