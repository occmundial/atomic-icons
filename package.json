{
  "name": "@occmundial/atomic-icons",
  "version": "1.7.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "concurrently 'cross-env NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" next dev -p 3002' 'yarn watch'",
    "watch": "watch 'yarn build:sprite && yarn build:font && yarn copy-files' ./icons",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir scripts --dir pages --dir components",
    "test": "jest --silent",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "prebuild:sprite": "rimraf dist && mkdirp dist && npx ts-node --project tsconfig.ci.json scripts/setPackage.ts",
    "build:sprite": "svg-sprite-generate -d ./icons -o ./dist/sprite.svg",
    "postbuild:sprite": "svgo dist/sprite.svg -o dist/atomic-icons.svg && rimraf dist/sprite.svg",
    "copy-files": "cp dist/atomic-icons.svg public/atomic-icons.svg && cp dist/font/* public/fonts/",
    "build:font": "ts-node --project tsconfig.ci.json scripts/createFont.ts",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@occmundial/atomic": "^3.0.0-beta.50",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-jss": "10.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "^0.14.0",
    "@types/babel__core": "^7.1.15",
    "@types/node": "^16.0.1",
    "@types/react": "17.0.13",
    "@types/write": "^2.0.1",
    "aws-sdk": "^2.1326.0",
    "axios": "^1.3.4",
    "babel-jest": "^27.0.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^27.0.6",
    "lint-staged": "^13.1.2",
    "mkdirp": "^2.1.4",
    "prettier": "^2.8.4",
    "replace-json-property": "^1.8.0",
    "rimraf": "^4.1.3",
    "semantic-release": "^19.0.2",
    "semantic-release-slack-bot": "^3.5.3",
    "svg-sprite-generator": "^0.0.7",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^10.0.6",
    "svgo": "^2.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.3.5",
    "watch": "^1.0.2",
    "write": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/occmundial/atomic-icons.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "20.16.0"
  }
}
